{
  "name": "game-of-life",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.0.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "styled-components": "^2.0.1"
  },
  "devDependencies": {
    "flow-bin": "^0.47.0",
    "node-sass-chokidar": "^0.0.2",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "flow": "flow",
    "build-css": "node-sass-chokidar src/ -o src/",
